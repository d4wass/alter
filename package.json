{
  "name": "alter",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test-watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "e2e": "ng e2e",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "prepare": "husky install",
    "lint": "ng lint && npm run lint:styles",
    "lint:styles": "stylelint src/**/*.html src/**/*.*css",
    "serve": "concurrently \"ng serve\" \"backend/node_modules/.bin/ts-node-dev backend/server.ts\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.1",
    "@angular/cdk": "^13.3.9",
    "@angular/common": "~13.1.1",
    "@angular/compiler": "~13.1.1",
    "@angular/core": "~13.1.1",
    "@angular/forms": "~13.1.1",
    "@angular/material": "^13.3.9",
    "@angular/platform-browser": "~13.1.1",
    "@angular/platform-browser-dynamic": "~13.1.1",
    "@angular/router": "~13.1.1",
    "@nestjs/jwt": "^8.0.1",
    "@ngneat/reactive-forms": "^4.1.0",
    "@ngrx/effects": "^13.2.0",
    "@ngrx/entity": "^13.2.0",
    "@ngrx/store": "^13.2.0",
    "@ngrx/store-devtools": "^13.2.0",
    "concurrently": "^7.1.0",
    "eslint-plugin-json": "^3.1.0",
    "normalize.css": "^8.0.1",
    "passport-jwt": "^4.0.0",
    "rxjs": "~7.4.0",
    "stylelint-scss": "^4.0.1",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.1.2",
    "@angular-eslint/builder": "13.1.0",
    "@angular-eslint/eslint-plugin": "13.1.0",
    "@angular-eslint/eslint-plugin-template": "13.1.0",
    "@angular-eslint/schematics": "13.1.0",
    "@angular-eslint/template-parser": "13.1.0",
    "@angular/cli": "~13.1.2",
    "@angular/compiler-cli": "~13.1.1",
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@ngneat/spectator": "^10.0.1",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.0",
    "@types/passport-jwt": "^3.0.6",
    "@typescript-eslint/eslint-plugin": "5.11.0",
    "@typescript-eslint/parser": "5.11.0",
    "cypress": "^9.5.2",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-jest": "^26.1.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-rxjs": "^5.0.2",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-preset-angular": "^11.1.1",
    "lint-staged": "^12.1.3",
    "ng-mocks": "^13.4.2",
    "prettier": "2.5.1",
    "prettier-eslint": "^13.0.0",
    "pretty-quick": "^3.1.2",
    "stylelint": "^14.1.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "typescript": "~4.5.4"
  },
  "lint-staged": {
    "{src,cypress}/**/*.ts": [
      "prettier --write",
      "eslint --fix"
    ],
    "{src,cypress}/**/*.{html,json,md}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "husky": {
    "pre-commit": "lint-staged",
    "pre-push": "npm run lint:staged"
  }
}